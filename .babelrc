{
  "presets": [
    [
      "@babel/env",
      {
        "useBuiltIns": "usage",
        "corejs": 3,
        "targets": {
          "esmodules": true
        }
	      //corejs: { version: 3, proposals: true }
      }
    ]
  ],
  "plugins": [
    ["@babel/transform-runtime", {
      "corejs":3,
      "useESModules": false
    }]
  ]
}
